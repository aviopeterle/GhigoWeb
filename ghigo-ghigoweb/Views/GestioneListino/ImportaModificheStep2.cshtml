    <tr>
        <td width="50%" style="text-align: left">@Html.ActionLink("Torna al passo precedente", "ImportaModifiche", null, new { @class = "btn pop orange" })</td>
        <td width="50%" style="text-align: right">@Html.ActionLink("Vai al passo successivo", "ImportaModificheStep3", null, new { @class = "btn pop blue" })</td>
    </tr>
</table>
